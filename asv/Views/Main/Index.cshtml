@using asv.Security;
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Конструктор запросов";    
}

@section scripts{    
    var connTimeout = @ViewBag.ConnTimeout, 
        itemsPerPage = @ViewBag.ItemsPerPage,
        minRequiredPasswordLength = @ViewData["minRequiredPasswordLength"],
        minRequiredUsernameLength = @ViewData["minRequiredUsernameLength"],
        ver = '@ViewData["version"]';

    Ext.define('Auser', @ViewBag.Auser );
}